<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Post" table="post">
    <id name="postId" type="integer" column="post_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="integer" column="user_id" nullable="false"/>
    <field name="title" type="text" column="title" length="65535" nullable="false"/>
    <field name="text" type="text" column="text" length="65535" nullable="false"/>
    <field name="date" type="date" column="date" nullable="false"/>
    <field name="published" type="boolean" column="published" nullable="false"/>
  </entity>
</doctrine-mapping>
